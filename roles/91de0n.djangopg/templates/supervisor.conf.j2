[program:{{ djangopg_gunicorn_procname }}]
command={{ djangopg_venv_path }}/bin/gunicorn -c gunicorn.conf.py -p gunicorn.pid {{ djangopg_proj_app }}.wsgi:application
directory={{ djangopg_proj_path }}
user={{ djangopg_user }}
autostart=true
stdout_logfile={{ djangopg_home }}/logs/{{ djangopg_proj_name }}_supervisor
autorestart=true
redirect_stderr=true
environment=LANG="{{ locale }}",LC_ALL="{{ locale }}",LC_LANG="{{ locale }}"
